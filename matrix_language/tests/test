declare:
#
# Описания матричных переменных
#
matrix: a,b, c("c_sparse_mtr.txt"),
D("D sparse mtr\"pseudo\".txt");
vector: v1,v2 ("../vector_sparse.txt");
rational:
number1(1), number2(-10 / 3), number3 ;
/*
* представлять как double в программе
*ldmlkdmfmsldmfldmsfmlsmdfklmsdlfml
dsfsdnfsdnkfnkjnfjnskfsdnfskdfnksdk
*//* jfjenwf ef */
float: f(0.99999999);
integer: i,j,k(10);
process:

a = b + c;
d = e+f;

a=(b+c info("сложили\n") ) * (E = D ^ num2) :rotate ; 
m=v1:rotate * v2; 
r=m[1,2];
info("всем привет\n"); 
r: write("лес.txt");
number1=r;
E: print;
E: write("hz.txt");
E: make_canonical;
a=b+c;

